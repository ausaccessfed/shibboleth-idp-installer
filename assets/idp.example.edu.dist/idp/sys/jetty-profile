JAVA_HOME=/usr/lib/jvm/java
JAVA=$JAVA_HOME/bin/java
JAVA_OPTIONS=" -server"
JETTY_HOME={{ jetty.home }}
JETTY_STATE={{ jetty.state_directory }}/jetty.state
JETTY_USER=jetty
JETTY_PORT=8080
JETTY_HOST=0.0.0.0
JETTY_START_LOG={{ jetty.base }}/log/jetty-start.log
JETTY_ARGS=jetty.logs={{ jetty.base }}/log
JETTY_BASE={{ jetty.base }}

